programa {
  funcao inicio() {
    //Algoritmo da tabuada
   //Declaração das variáveis
   inteiro multiplicando, i, resultado

   escreva("TABUADA 2°\n\n")

   escreva("Digitando o multiplicando: ")
   leia (multiplicando)

   para(1 = 0; i <= 10; i++){
    resultado = multiplicando * i
    escreva(multiplicando, "x", i, "=", resultado, "\n")
   }
  }
}
