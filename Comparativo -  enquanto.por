programa {
  funcao inicio() {
  //Comprando o uso de enquanto e faça-enquanto
  logico condicao  
  condicao = falso
  enquanto(condicao){
    escreva("Mensagem dentro do Enquanto\n")
  }
  escreva("Fora do Enquanto\n")
  condicao = falso
  faca{
    escreva("Mensagem dentro do Faça-Enquanto\n")
  } enquanto(condicao)
  escreva("Fora do Faça-enquanto\n")
  }
}
